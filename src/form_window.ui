<?xml version='1.0' encoding='UTF-8'?>
<interface>
    <template class="form-window" parent="AdwApplicationWindow">
        <property name="default-height">400</property>
        <property name="default-width">400</property>
        <property name="title">Form Window</property>
        <child>
            <object class="GtkListBox">
                <child>
                    <object class="GtkLabel">
                        <property name="label">Outside label</property>
                    </object>
                </child>
                <child>
                    <object class="AdwPreferencesGroup">
                        <property name="title">Preference Group</property>
                        <child type="header-suffix">
                          <object class="GtkLabel">
                              <property name="label">Header Suffix</property>
                          </object>
                        </child>
                        <child>
                            <object class="GtkListBox" id="list_box">
                                <style>
                                    <class name="boxed-list"/>
                                </style>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
  </template>
</interface>